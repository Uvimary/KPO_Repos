#include <iostream> 
#include <sstream> 

using namespace std;

string check(string s) {
    string word = "hello";
    int j = 0;

    for (int i = 0; i < s.length(); ++i) {
        if (s[i] == word[j]) {
            j++;
        }
        if (j == 5) {
            return "YES";
        }
    }
    return "NO";
}

void simpleTest() {

    string input = "vghkkfellnfhouc";
    string expectedOutput = "YES";

    string output = check(input)


    // Проверка результата теста
    if (output == expectedOutput) {
        cout << "Тест пройден" << endl;
    }
    else {
        cout << "Тест не пройден" << endl;
        cout << "Ожидаемый результат: " << expectedOutput << endl;
        cout << "Полученный результат: " << output << endl;
    }
}

int main() {
    simpleTest();
    return 0;
}
